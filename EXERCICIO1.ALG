Algoritmo "EXERCICIO1"

var
 n1,n2,soma,diferenca,produto,media: real


inicio
 escreva (" entre com o primeiro numero: ")
 leia (n1)

 escreva (" entre com o segundo numero: ")
 leia (n2)

 escreva(" soma:", n1+n2)
 leia (soma)
 escreva(" diferença:", n1-n2)
 leia (diferenca)
 escreva(" produto:", n2*n1)
 leia (produto)
 escreva(" media:", (n1+n2)/2)
 leia (media)


fimalgoritmo

